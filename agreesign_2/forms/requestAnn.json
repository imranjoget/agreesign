  .toolbar .tool .tool-button-danger:hover {\r\n    background-color: #c82333;\r\n  }\r\n\r\n  .toolbar .tool .tool-button-info {\r\n    border-radius: 0.2rem;\r\n    background-color: #17a2b8;\r\n    color: #fff;\r\n    cursor: pointer;\r\n  }\r\n\r\n  .toolbar .tool .tool-button-info:hover {\r\n    background-color: #117a8b;\r\n  }\r\n\r\n  .toolbar .tool .tool-button-light {\r\n    border-radius: 0.2rem;\r\n    background-color: #f8f9fa;\r\n    color: #212529;\r\n    cursor: pointer;\r\n  }\r\n\r\n  .toolbar .tool .tool-button-light:hover {\r\n    background-color: #e2e6ea;\r\n  }\r\n\r\n  .signingSpots {\r\n    margin: 16px 24px;\r\n  }\r\n\r\n  .grey-background #SignatureSidebar>div.signingSpots>div>a>span {\r\n    margin-left: 25px;\r\n    font-size: 18px;\r\n  }\r\n\r\n  .grey-background #SignatureSidebar>div.signingSpots>div>a>input[type=checkbox] {\r\n    width: 5px;\r\n    height: 5px;\r\n    position: absolute;\r\n    top: 35px;\r\n    left: 40px;\r\n  }\r\n\r\n  .list-group-item {\r\n    padding: 1rem 1.25rem;\r\n  }\r\n\r\n  .option__label__title,\r\n  .option__title {\r\n    font-size: 18px;\r\n    font-weight: 500;\r\n    color: #212529;\r\n    letter-spacing: .3px;\r\n    line-height: 24px;\r\n    text-transform: uppercase;\r\n    margin-bottom: 15px;\r\n  }\r\n\r\n  .option__panel__bottom {\r\n    margin-bottom: 0;\r\n    padding: 21px;\r\n    background-color: #fff;\r\n    bottom: 0;\r\n    width: 450px;\r\n    position: absolute;\r\n    height: 140px;\r\n  }\r\n\r\n  #processTask {\r\n    width: 100%;\r\n    height: 100%;\r\n    background-color: rgb(22, 119, 255);\r\n    color: #fff;\r\n    font-size: 28px;\r\n    font-weight: 500;\r\n  }\r\n\r\n  #processTask span {\r\n    height: 24px;\r\n    display: -ms-inline-flexbox;\r\n    display: inline-flex;\r\n    -ms-flex-align: center;\r\n    align-items: center;\r\n    -ms-flex-pack: center;\r\n    justify-content: center;\r\n    padding: 40px;\r\n    position: relative;\r\n    vertical-align: bottom;\r\n    margin-left: 8px;\r\n  }\r\n\r\n  #processTask span,\r\n  #settingsRefresh span {\r\n    display: -ms-flexbox;\r\n    display: flex;\r\n    min-height: inherit;\r\n  }\r\n\r\n  .toolbar .tool.editButton {\r\n    float: right;\r\n    display: none;\r\n  }\r\n<\/style>\r\n<div class=\"grey-background\">\r\n    <div id=\"SignatureInfo\" class=\"SignatureInfo\"><p>To add signature location, simply click on the designated area below.<\/p><\/div>\r\n  <div class=\"toolbar\">\r\n    <div class=\"tool\">\r\n      <button class=\"tool-button\" title=\"Free Hand\" onclick=\"enableSelector(event)\">\r\n        <i class=\"fas fa-arrows-alt\"><\/i>\r\n      <\/button>\r\n    <\/div>\r\n    <div class=\"tool\">\r\n      <button class=\"tool-button\" title=\"Add Signature\" onclick=\"enableRectangle(event)\">\r\n        <i class=\"fas fa-signature\"><\/i>\r\n      <\/button>\r\n    <\/div>\r\n    <div class=\"tool\">\r\n      <button class=\"tool-button-danger\" title=\"Delete selected\" onclick=\"deleteSelectedObject(event)\">\r\n        <i class=\"far fa-trash-alt\"><\/i>\r\n      <\/button>\r\n    <\/div>\r\n    <div class=\"tool doneButton\">\r\n      <a class=\"tool-button\" title=\"Done\" onclick=\"closePanel('test')\">Save<\/a>\r\n    <\/div>\r\n    <div class=\"tool editButton\">\r\n      <a class=\"tool-button\" title=\"Edit\" onclick=\"editPanel('test')\">Edit<\/a>\r\n    <\/div>\r\n  <\/div>\r\n  <div class=\"content\">\r\n    <div id=\"pdf-container\" class=\"pdf-container\"><\/div>\r\n  <\/div>\r\n<\/div>\r\n<script src=\"#appResource.fabric.min.js#\"><\/script>\r\n<script src=\"#appResource.pdf.js#\"><\/script>\r\n<script>\r\n  pdfjsLib.GlobalWorkerOptions.workerSrc = \"#appResource.pdf.worker.js#\";\r\n<\/script>\r\n<script src=\"#appResource.pdfannotate.js#\"><\/script>\r\n<script type=\"text/javascript\">\r\n  // standard options type:\r\n  // If absolute URL from the remote server is provided, configure the CORS header on that server.\r\n  var pdf;\r\n  var url = '#request.domainURL#/jw/web/client/app/#appDef.appId#/#appDef.version#/form/download/request/#form.ags_record.id#/#form.ags_record.agreement#.';\r\n  var zoomScale = 1.2;\r\n  var outputJson = {};\r\n  var selectedObjects = new Set();\r\n  var originalSize;\r\n  let signatureCount = 0;\r\n  var screenWidth = window.innerWidth;\r\n\r\n    if (screenWidth <= 600) {\r\n        zoomScale = 0.35;\r\n    } else if(screenWidth <= 991 ) {\r\n        zoomScale = 0.7;\r\n    } else if(screenWidth >= 992 ) {\r\n        zoomScale = 1.2;\r\n    }\r\n\r\n  function renderPDF() {\r\n    pdf = new PDFAnnotate('pdf-container', url, {\r\n      onPageUpdated(page, oldData, newData) {\r\n      },\r\n      ready() {\r\n        const jsonData = FormUtil.getField(\"annotation_data\").val();\r\n        if (jsonData) {\r\n          const parsedData = JSON.parse(jsonData);\r\n          pdf.loadFromJSON(parsedData);\r\n          parsedData.pages.forEach((page) => {\r\n\t\t\tpage.objects.forEach((object) => {\r\n\t\t\t  signatureCount++\r\n\t\t\t});\r\n\t\t  });\r\n\t\t  FormUtil.getField(\"signature_count\").val(signatureCount);\r\n        }\r\n      },\r\n      scale: zoomScale,\r\n      pageImageCompression: 'MEDIUM', // FAST, MEDIUM, SLOW(Helps to control the new PDF file size)\r\n    });\r\n  }\r\n\r\n  function changeActiveTool(event) {\r\n    var element = $(event.target).hasClass('tool-button') ? $(event.target) : $(event.target).parents('.tool-button').first();\r\n    $('.tool-button.active').removeClass('active');\r\n    $(element).addClass('active');\r\n  }\r\n\r\n  function enableSelector(event) {\r\n    $('#SignatureInfo').hide();\r\n    event.preventDefault();\r\n    changeActiveTool(event);\r\n    pdf.enableSelector();\r\n  }\r\n\r\n  function enablePencil(event) {\r\n    event.preventDefault();\r\n    changeActiveTool(event);\r\n    pdf.enablePencil();\r\n  }\r\n\r\n  function enableAddText(event) {\r\n    event.preventDefault();\r\n    changeActiveTool(event);\r\n    pdf.enableAddText(function() {\r\n      $('.tool-button').first().find('i').click();\r\n    });\r\n  }\r\n\r\n  function enableAddArrow(event) {\r\n    event.preventDefault();\r\n    changeActiveTool(event);\r\n    pdf.enableAddArrow(function() {\r\n      $('.tool-button').first().find('i').click();\r\n    });\r\n  }\r\n\r\n  function addImage(event) {\r\n    event.preventDefault();\r\n    pdf.addImageToCanvas();\r\n  }\r\n\r\n  function enableRectangle(event) {\r\n    event.preventDefault();\r\n    $('#SignatureInfo').show()\r\n    changeActiveTool(event);\r\n    pdf.enableRectangle(function() {\r\n      $('.tool-button').first().find('i').click();\r\n    });\r\n  }\r\n\r\n  function undo(event) {\r\n    event.preventDefault();\r\n    pdf.undo();\r\n  }\r\n\r\n  function redo(event) {\r\n    event.preventDefault();\r\n    pdf.redo();\r\n  }\r\n\r\n  function deleteSelectedObject(event) {\r\n    event.preventDefault();\r\n    pdf.deleteSelectedObject();\r\n  }\r\n\r\n  function savePDF() {\r\n    // pdf.savePdf();\r\n    pdf.savePdf('output.pdf'); // save with given file name\r\n  }\r\n\r\n  function clearPage() {\r\n    pdf.clearActivePage();\r\n  }\r\n\r\n  function showPdfData() {\r\n    let objectNumber = 0;\r\n    var annotateData;\r\n    pdf.serializePdf(function(string) {\r\n      const parsedJson = JSON.parse(string);\r\n      parsedJson.pages.forEach((page) => {\r\n        page.objects.forEach((object) => {\r\n          objectNumber++;\r\n        });\r\n      });\r\n      annotateData = JSON.stringify(JSON.parse(string));\r\n      FormUtil.getField(\"signature_count\").val(objectNumber);\r\n      FormUtil.getField(\"annotation_data\").val(annotateData);\r\n    });\r\n  }\r\n\r\n  function showVal(a) {\r\n    pdf.serializePdf(function(string) {\r\n      outputJson = JSON.parse(string);\r\n    });\r\n    zoomScale = Number(a);\r\n    renderPDF();\r\n  }\r\n  // Get a reference to the select box\r\n  var selectBox = document.getElementById('signature-spot');\r\n  // Function to select the next option\r\n  function selectNext() {\r\n    var selectedIndex = selectBox.selectedIndex;\r\n    if (selectedIndex < selectBox.options.length - 1) {\r\n      selectBox.selectedIndex = selectedIndex + 1;\r\n      $(selectBox).trigger('change');\r\n    }\r\n  }\r\n  // Function to select the previous option\r\n  function selectPrevious() {\r\n    var selectedIndex = selectBox.selectedIndex;\r\n    if (selectedIndex > 0) {\r\n      selectBox.selectedIndex = selectedIndex - 1;\r\n      $(selectBox).trigger('change');\r\n    }\r\n  }\r\n\r\n  function updateHiddenField(value) {\r\n    $('#SignSelected').text(selectedObjects.size + \" /\" + originalSize);\r\n    FormUtil.getField(\"selectedSignature\").val(value);\r\n  }\r\n\r\n  function closePanel(value) {\r\n    showPdfData();\r\n    $('#section-actions').show();\r\n    $('.tool').hide();\r\n    $('.editButton').show();\r\n    $(\".grey-background .content\").css(\"filter\", \"brightness(0.5)\");\r\n  }\r\n\r\n  function editPanel(value) {\r\n    $('#section-actions').hide();\r\n    $('.tool').show();\r\n    $('.editButton').hide();\r\n    $(\".grey-background .content\").css(\"filter\", \"brightness(1)\");\r\n  }\r\n  $(function() {\r\n    $('#signature-spot').on('change', function() {\r\n      var pageNumber = parseInt(this.value);\r\n      var $elementToScroll = $(\"#page-\" + pageNumber + \"-canvas\");\r\n      var divElement = document.getElementById('pdf-container');\r\n      var currentScrollPosition = divElement.scrollTop;\r\n      // Calculate the position to scroll to (middle of the element)\r\n      var scrollPosition = $elementToScroll.offset().top;\r\n      // Scroll to the calculated position\r\n      $('#pdf-container').animate({\r\n        scrollTop: currentScrollPosition + scrollPosition - 150\r\n      }, 500);\r\n    });\r\n  });\r\n  renderPDF();\r\n\r\n  fabric.LabeledRect = fabric.util.createClass(fabric.Rect, {\r\n    type: 'labeledRect',\r\n    initialize: function(options) {\r\n      options || (options = {});\r\n      var label = 'Signature';\r\n      this.callSuper('initialize', options);\r\n      this.set('label', label || '');\r\n    },\r\n    toObject: function() {\r\n      return fabric.util.object.extend(this.callSuper('toObject'), {\r\n        label: this.get('label')\r\n      });\r\n    },\r\n    _render: function(ctx) {\r\n      this.callSuper('_render', ctx);\r\n      ctx.font = '20px Helvetica';\r\n      ctx.fillStyle = 'rgb(22, 119, 255)';\r\n      ctx.textAlign = 'center'; // Center text h\r\n      ctx.textBaseline = 'middle';\r\n      var textY = -this.height / 2 + this.height / 2;\r\n      ctx.fillText(this.label, 0, textY);\r\n    }\r\n  });\r\n  fabric.LabeledRect.fromObject = function(object, callback) {\r\n    return fabric.Object._fromObject('LabeledRect', object, callback);\r\n  }\r\n<\/script>"
                        }
